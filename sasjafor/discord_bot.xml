<?xml version="1.0" encoding="utf-8"?>
<Container>
  <TemplateURL>https://raw.githubusercontent.com/sasjafor/docker-templates/master/sasjafor/discord_bot.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2017-08-16</Date>
  <Changes>[center][img width='300px' src='https://raw.githubusercontent.com/sasjafor/docker-templates/master/templates/img/discord_bot.png'][/center]
  
  [center][font size=4] Change Log[/font][/center]
  </Changes>
  <Name>DiscordBot</Name>
  <Support></Support>
  <Description> [br]
  [br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
  [b]/usr/src/app[/b] : Mapping for bot files [br]
  </Description>
  <Project></Project>
  <Overview></Overview>
  <Registry>https://hub.docker.com/r/sasjafor/discord_bot-docker/</Registry>
  <Repository>sasjafor/discord_bot-docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Network Default="bridge">bridge</Network>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
