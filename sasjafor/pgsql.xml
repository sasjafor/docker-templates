<?xml version="1.0" encoding="utf-8"?>
<Container>
  <TemplateURL>https://raw.githubusercontent.com/sasjafor/docker-templates/master/sasjafor/pgsql.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2017-11-30</Date>
  <Changes>[center][img width='300px' src='https://raw.githubusercontent.com/sasjafor/docker-templates/master/templates/img/pgsql.png'][/center]
  
  [center][font size=4] Change Log[/font][/center]
  </Changes>
  <Name>pgsql</Name>
  <Support></Support>
  <Description> [br]
  [br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]
  [b]/config[/b] : Mapping for server relevant files[br]
  </Description>
  <Project></Project>
  <Registry>https://hub.docker.com/r/sasjafor/pgsql-docker/</Registry>
  <Repository>sasjafor/pgsql-docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
	<Variable>
      <Name>POSTGRES_PASSWORD</Name>
      <Value>password</Value>
    </Variable>
	<Variable>
      <Name>SERVER_HOSTNAME</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>WEBMASTER_PASSWORD</Name>
      <Value></Value>
    </Variable>
	<Variable>
      <Name>MAILMAN_PASSWORD</Name>
      <Value></Value>
    </Variable>
	<Variable>
      <Name>MAIN_ACCOUNT_USERNAME</Name>
      <Value></Value>
    </Variable>
	<Variable>
      <Name>MAIN_ACCOUNT_PASSWORD</Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/var/lib/postgresql/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://raw.githubusercontent.com/sasjafor/docker-templates/master/templates/img/pgsql.png</Icon>
  <BaseImage>postgres</BaseImage>
</Container>
